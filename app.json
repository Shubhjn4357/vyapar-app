{
  "expo": {
    "name": "vyapar-app",
    "slug": "vyapar-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "vyaparapp",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.shubhjn.vyaparapp",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#000000"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "androidNavigationBar":{
      "visible":"immersive",
      "barStyle":"dark-content",
      "backgroundColor":"#000000"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "9d5ccb7c-830d-4366-a64b-aa5fa53377bd"
      }
    }
  }
}
